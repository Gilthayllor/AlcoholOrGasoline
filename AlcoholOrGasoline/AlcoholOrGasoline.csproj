<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>$(AssemblyName)</PackageId>
		<VersionPrefix>1</VersionPrefix>
		<Version>$(VersionPrefix).0.2</Version>
		<Description>This project calculates whether it is cheaper to fill up with alcohol or gasoline.</Description>
		<Summary>This project calculates whether it is cheaper to fill up with alcohol or gasoline.</Summary>
		<Authors>Gilthayllor Sousa</Authors>
		<Company>Gilthayllor Sousa</Company>
		<RepositoryUrl>https://github.com/Gilthayllor/AlcoholOrGasoline</RepositoryUrl>
		<PackageProjectUrl>https://github.com/Gilthayllor/AlcoholOrGasoline</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageTags>alcohol;gasoline</PackageTags>
		<Title>Alcohol or Gasoline</Title>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
	</PropertyGroup>
</Project>
